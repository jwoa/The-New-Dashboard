{
  "name": "tns-dashboard",
  "productName": "The New Dashboard",
  "version": "0.0.1",
  "description": "A desktop application for students who attend The New School.",
  "main": "main.js",
  "scripts": {
    "build": "electron-packager . tns-dashboard",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=images/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . tns-dashboard --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Electron Tutorial App\"",
    "package-linux": "electron-packager . tns-dashboard --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jwoa/The-New-Dashboard.git"
  },
  "author": "Jason Agyekum <jason@seasonsix.co>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jwoa/The-New-Dashboard/issues"
  },
  "homepage": "seasonsix.co/tns",
  "devDependencies": {
    "asar": "^0.14.3",
    "electron": "2.0.4",
    "electron-packager": "^12.1.0"
  },
  "dependencies": {
    "express": "^4.16.3"
  }
}
